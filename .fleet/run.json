{
    "configurations": [
        {
            "type": "command",
            "name": "Launch app",
            "workingDir": "$PROJECT_DIR$",
            "program": "adb",
            "args": [
                "shell",
                "am",
                "start",
                "-n",
                "com.dede.android_eggs/.views.main.EasterEggsActivity"
            ],
        },
        {
            "type": "command",
            "name": "Update changelogs",
            "workingDir": "$PROJECT_DIR$/script/changelogs",
            "program": "python3",
            "args": [
                "changelogs.py"
            ],
        },
    ],
}
